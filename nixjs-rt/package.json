{
  "name": "nixjs-rt",
  "version": "0.0.1",
  "description": "A library that implements Nix language semantics in JS.",
  "main": "dist/lib.mjs",
  "scripts": {
    "build": "tsup ./src/lib.ts --format esm --sourcemap",
    "build-watch": "tsup ./src/lib.ts --format esm --sourcemap --watch",
    "fmt": "prettier --write .",
    "fmt-check": "prettier --check .",
    "test": "jest --color",
    "test-watch": "jest --color --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/urbas/nixjs-rt.git"
  },
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/urbas/nixjs-rt/issues"
  },
  "homepage": "https://github.com/urbas/nixjs-rt#readme",
  "devDependencies": {
    "@jest/globals": "29.7.0",
    "jest": "29.7.0",
    "prettier": "3.2.5",
    "ts-jest": "29.1.2",
    "tsup": "^8.0.2",
    "typescript": "5.3.3"
  },
  "files": [
    "dist/**"
  ]
}
